}

/*QTabWidget style*/
QWidget#receiveWidget,
QWidget#historyWidget,
QWidget#helpWidget,
QWidget#setWidget,
QWidget#page_fileWidget_main,
QWidget#page_fileWidget_new
QWidget#widget_receve_control
{
        background-color: rgb(255,255,255);
}

QWidget#widget_history_display{
    border: 2px solid qlineargradient(spread:pad, x1:0.494318, y1:0, x2:0.54, y2:1, stop:0.0625 rgba(213, 213, 213, 255), stop:1 rgba(184, 184, 184, 255));
}

QStackedWidget#stackedWidget_receiveWidget
{
    border: 2px solid qlineargradient(spread:pad, x1:0.494318, y1:0, x2:0.54, y2:1, stop:0.0625 rgba(213, 213, 213, 255), stop:1 rgba(184, 184, 184, 255));
}
